{
  "name": "CSSModulesExample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "tsc": "tsc --project . --noEmit",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "eslint . && stylelint 'src/**/*.@(css|scss)'",
    "web": "webpack-dev-server --mode development",
    "ios": "react-native run-ios",
    "android": "react-native run-android"
  },
  "dependencies": {
    "es6-shim": "^0.35.3",
    "react": "16.5.0",
    "react-art": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-fontawesome": "^1.6.1",
    "react-native": "0.57.0",
    "react-native-fontawesome": "^5.7.0",
    "react-native-web": "^0.8.9",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-native": "kristerkari/react-native-types-for-css-modules#v0.56.18",
    "autoprefixer": "^9.1.3",
    "babel-jest": "23.4.2",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-react-native-classname-to-style": "^1.2.1",
    "babel-plugin-react-native-platform-specific-extensions": "^1.1.1",
    "babel-preset-react": "^7.0.0-beta.3",
    "css-loader": "^1.0.0",
    "eslint": "^5.4.0",
    "eslint-plugin-css-modules": "^2.7.5",
    "file-loader": "^2.0.0",
    "metro-react-native-babel-preset": "^0.45.0",
    "node-sass": "^4.9.3",
    "postcss": "^7.0.2",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "3.1.3",
    "react-native-typed-css-transformer": "^0.10.0",
    "react-native-typed-sass-transformer": "^0.10.0",
    "react-test-renderer": "16.4.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "stylelint": "^9.6.0",
    "stylelint-config-react-native-css-modules": "^2.0.0",
    "stylelint-react-native": "^2.0.0",
    "ts-loader": "^4.5.0",
    "typescript": "^3.0.1",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11",
    "Android >= 4.4",
    "iOS >= 8"
  ],
  "jest": {
    "preset": "react-native"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "css-modules"
    ],
    "extends": [
      "plugin:css-modules/recommended"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-react-native-css-modules",
    "rules": {
      "selector-class-pattern": "^[a-z][a-zA-Z0-9]*$",
      "declaration-block-no-duplicate-properties": true,
      "no-duplicate-selectors": true,
      "no-extra-semicolons": true,
      "no-eol-whitespace": true,
      "no-missing-end-of-source-newline": true
    }
  }
}
